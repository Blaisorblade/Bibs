}

@inproceedings{Hinze2000memo,
  title = {Memo {{Functions}}, {{Polytypically}}!},
  booktitle = {Proceedings of the 2nd {{Workshop}} on {{Generic Programming}}, {{Ponte}} De},
  author = {Hinze, Ralf},
