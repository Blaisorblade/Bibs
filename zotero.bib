@inproceedings{Abel2014Formalized,
  title = {A {{Formalized Proof}} of {{Strong Normalization}} for {{Guarded Recursive Types}}},
  booktitle = {Programming {{Languages}} and {{Systems}}},
  author = {Abel, Andreas and Vezzosi, Andrea},
  year = {2014},
