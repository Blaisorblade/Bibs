  booktitle = {Applied {{Semantics}}},
  author = {Dybjer, Peter and Filinski, Andrzej},
  editor = {Barthe, Gilles and Dybjer, Peter and Pinto, Lu{\'i}s and Saraiva, Jo{\~a}o},
