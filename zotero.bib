  title = {Pure {{Subtype Systems}}},
  booktitle = {Proceedings of the 37th {{Annual ACM SIGPLAN}}-{{SIGACT Symposium}} on {{Principles}} of {{Programming Languages}}},
  author = {Hutchins, DeLesley S.},
