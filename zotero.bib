  series = {{{OOPSLA}} '87}
}

@inproceedings{Maher2005Herbrand,
  title = {Herbrand Constraint Abduction},
  booktitle = {20th {{Annual IEEE Symposium}} on {{Logic}} in {{Computer Science}} ({{LICS}}' 05)},
