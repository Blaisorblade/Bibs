@inproceedings{Ricketts2014automating,
  title = {Automating {{Formal Proofs}} for {{Reactive Systems}}},
  booktitle = {Proceedings of the 35th {{ACM SIGPLAN Conference}} on {{Programming Language Design}} and {{Implementation}}},
